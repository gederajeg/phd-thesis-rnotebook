  pages = {15--33},
  keywords = {COGNITION & culture,CROSS-cultural studies,INTELLECT -- Social aspects,METAPHOR -- Social aspects,SOCIAL epistemology,VISUAL literacy,VISUAL perception -- Cross-cultural studies}
