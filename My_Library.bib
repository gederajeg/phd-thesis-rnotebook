  keywords = {attraction and reliance,COGNITIVE analysis,collostructional analysis,entrenchment,FISHER exact test,FOREIGN associations; institutions; etc.,LANGUAGE & languages,lexicogrammatical associations,ODDS ratio,p-value,quantitative linguistics,STATISTICAL hypothesis testing},
  file = {Schmid_Küchenhoff_2013_Collostructional analysis and other ways of measuring lexicogrammatical.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Schmid_Küchenhoff_2013_Collostructional analysis and other ways of measuring lexicogrammatical.pdf:application/pdf}
