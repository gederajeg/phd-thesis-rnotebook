@article{papafragou_source-goal_2010,
  title = {Source-{{Goal Asymmetries}} in {{Motion Representation}}: {{Implications}} for {{Language Production}} and {{Comprehension}}},
  volume = {34},
  issn = {03640213},
  doi = {10.1111/j.1551-6709.2010.01107.x},
  shorttitle = {Source-{{Goal Asymmetries}} in {{Motion Representation}}},
  abstract = {Recent research has demonstrated an asymmetry between the origins and endpoints of motion events, with preferential attention given to endpoints rather than beginnings of motion in both language and memory. Two experiments explore this asymmetry further and test its implications for language production and comprehension. Experiment 1 shows that both adults and 4-year-old children detect fewer within-category changes in source than goal objects when tested for memory of motion events; furthermore, these groups produce fewer references to source than goal objects when describing the same motion events. Experiment 2 asks whether the specificity of encoding source/goal relations differs in both spatial memory and the comprehension of novel spatial vocabulary. Results show that endpoint configuration changes are detected more accurately than source configuration changes by both adults and young children. Furthermore, when interpreting novel motion verbs, both age groups expect more fine-grained lexical distinctions in the domain of endpoint configurations compared to that of source configurations. These studies demonstrate that a cognitive-attentional bias in spatial representation and memory affects both the detail of linguistic encoding during the use of spatial language and the specificity of hypotheses about spatial referents that learners build during the acquisition of the spatial lexicon.},
  timestamp = {2014-03-30T14:11:50Z},
  number = {6},
  journaltitle = {Cognitive Science},
  shortjournal = {Cognitive Science},
  author = {Papafragou, Anna},
  urldate = {2014-03-30},
  date = {2010-08},
  pages = {1064--1092},
  keywords = {ASYMMETRY (Linguistics),COMPREHENSION (Theory of knowledge),language acquisition,SENSITIVITY & specificity (Statistics),SPATIAL ability}
}

