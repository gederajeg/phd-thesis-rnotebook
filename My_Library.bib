  date = {2013-04},
  pages = {167--198},
  keywords = {diachrony;,ENGLISH language -- United States,GOOGLE (Web resource),granularity,HISTORICAL linguistics,LINGUISTS,monitor corpus,SEARCH engines,tools,web as corpus},
  file = {Renouf_Kehoe_2013_Filling the gaps.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Renouf_Kehoe_2013_Filling the gaps.pdf:application/pdf}
}

