  pages = {88--116},
  keywords = {Cognitive grammar,computational construction grammar,constructional language processing,Construction grammar,formalization,Generative grammar,Language & languages -- Cognitive processing,Linguistics -- History},
  file = {Van Trijp_2013_A comparison between Fluid Construction Grammar and Sign-Based Construction.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Van Trijp_2013_A comparison between Fluid Construction Grammar and Sign-Based Construction.pdf:application/pdf}
}

