  pages = {423--466},
  keywords = {(adjectival) passive,collostruction analysis,construction grammar,ENGLISH language,grammaticalization,LEXICAL grammar,SEMANTICS,un-participle construction,unpassive,VERBS (Grammar)},
  file = {Schönefeld_2015_A constructional analysis of English un-participle constructions.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Schönefeld_2015_A constructional analysis of English un-participle constructions.pdf:application/pdf}
