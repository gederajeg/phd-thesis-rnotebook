  date = {2012-06},
  pages = {49--89},
  keywords = {corpora (linguistics),corpus linguistics,polysemy,propulsion verbs,quantitative corpus linguistics,quantitative linguistics,quantitative methods,radial category,SENTENCES (Grammar),Spanish,SYNONYMS,synonymy,throw verbs,VERBS (Grammar)},
  file = {Skallman_2012_Throwing results.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Skallman_2012_Throwing results.pdf:application/pdf}
