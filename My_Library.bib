  urldate = {2016-11-06},
  date = {2007-09-10},
  pages = {733--764},
  keywords = {abstract concepts,Conceptual Metaphor Theory,embodied cognition,embodied simulation,experimental methods,Language understanding,Linguistics,Mental simulation,METAPHOR -- Psychological aspects,Metaphor Psychological aspects,Perception,Psychology,SEMANTICS,Spatial cognition},
  file = {Bergen et al_2007_Spatial and Linguistic Aspects of Visual Imagery in Sentence Comprehension.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Bergen et al_2007_Spatial and Linguistic Aspects of Visual Imagery in Sentence Comprehension.pdf:application/pdf}
