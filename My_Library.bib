  pages = {190--220},
  keywords = {argument structure,collostructional analysis,Comparative Linguistics,constructional semantics,corpus-based,ditransitive,double object construction,Dutch,English language education,INFORMATION theory,multidimensional approach,NETHERLANDS,SEMANTICS,VERBS (Grammar)},
  file = {Colleman_2009_The semantic range of the Dutch double object construction.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Colleman_2009_The semantic range of the Dutch double object construction.pdf:application/pdf}
