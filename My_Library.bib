@article{zhong_clean_2010,
  title = {A Clean Self Can Render Harsh Moral Judgment},
  volume = {46},
  issn = {0022-1031},
  doi = {10.1016/j.jesp.2010.04.003},
  abstract = {Physical cleanliness has many medical benefits, such as protection from the dangers of contagion. We explore a potential unintended consequence of cleanliness. Given the metaphorical association between physical cleanliness and moral purity (Zhong \&amp; Liljenquist, 2006), we contend that a clean self may also be linked to a virtuous self. This enhanced moral self-perception can in turn license harsher moral judgment. Three experiments found that cleanliness, whether induced via physical cleansing or through a visualization task, licensed severe judgment on morally contested issues such as abortion and pornography. Further, we found that an inflated moral self mediated the relationship between cleanliness and moral judgment. These results provide unique insight to the social significance of cleanliness and may have important implications for discrimination and prejudice.},
