@article{rahkonen_aspects_2006,
  title = {Some Aspects of Topicalization in Active {{Swedish}} Declaratives: A Quantitative Corpus Study},
  volume = {44},
  issn = {00243949},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=hlh&AN=20567588&site=ehost-live},
  shorttitle = {Some Aspects of Topicalization in Active {{Swedish}} Declaratives},
  abstract = {In Swedish declaratives, clause-initial NP Fin NP…strings are structurally ambiguous. Though the prefinite argument NP normally occupies the role of the subject, in some circumstances the interpretation OVS is preferred over the basic SVO word order. Thus, the initial NP can also be analyzed as a nonsubject, that is, as a direct or indirect object, the object of a preposition, a predicate noun, or a predicate adjective. In this article, the consequences of this ambiguity for the production of NP Fin NP…sequences are studied More specifically, the influence of word order, inflectional morphology, content factors, and anaphoric reference on the topicalization of nonsubject NPs in two written corpora are discussed. A necessary condition for the production of an OVS structure is that the NP Fin NP… string cannot be interpreted as an SVO structure. In the analyzed texts, formal markers have only a weak correlation with the occurrence of OVS sequences as compared to the effect of meaning. A further factor influencing the probability of an OVS order is the textual continuity of the nonsubject NP. Most of the topicalized nonsubjects in the corpora are NPs referring to an antecedent in the left context.},
  timestamp = {2014-03-30T15:55:29Z},
  number = {1},
  journaltitle = {Linguistics},
  shortjournal = {Linguistics},
  author = {Rahkonen, Matti},
  urldate = {2014-03-30},
  date = {2006-01},
  pages = {23--55},
  keywords = {COMPARATIVE grammar,LANGUAGE & languages,linguistics,PHILOLOGY}
}

@article{utsumi_indirect_2011,
  title = {Indirect {{Categorization}} as a {{Process}} of {{Predicative Metaphor Comprehension}}},
  volume = {26},
  issn = {10926488},
  doi = {10.1080/10926488.2011.609120},
  abstract = {In this article, we address the problem of how people understand predicative metaphors such as “The rumor flew through the office,” and argue that predicative metaphors are understood as indirect (or two-stage) categorizations. In the indirect categorization process, the verb (e.g., fly) of a predicative metaphor evokes an intermediate entity, which in turn evokes a metaphoric category of actions or states (e.g., “to spread rapidly and soon disappear”) to be attributed to the target noun (e.g., rumor), rather than directly evoking a metaphoric category as argued by Glucksberg's (2001) categorization theory. We test our argument using two experiments, namely, offline comprehension and online priming. The offline comprehension experiment showed that interpretation of predicative metaphors had greater overlap with words indirectly associated with the verb than those directly associated with the verb. The online priming experiment demonstrated that indirectly associated words were activated during predicative metaphor comprehension, but directly associated words were not. These results provide convergent evidence for our argument, and thus the psychological validity of two-stage categorization as a process of predicative metaphor comprehension was confirmed.},
  timestamp = {2014-03-31T03:37:03Z},
  number = {4},
  journaltitle = {Metaphor \& Symbol},
  shortjournal = {Metaphor \& Symbol},
  author = {Utsumi, Akira and Sakamoto, Maki},
  urldate = {2014-03-31},
  date = {2011-10},
  pages = {299--313},
  keywords = {abstract thought,CATEGORIZATION (Linguistics),cognitive grammar,metaphor,MOTION,VERBS (Grammar)}
}

