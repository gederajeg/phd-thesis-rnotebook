  file = {Jansegers et al_2015_The polysemy of the Spanish verb sentir.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Jansegers et al_2015_The polysemy of the Spanish verb sentir.pdf:application/pdf}
