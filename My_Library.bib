  keywords = {basic level vs. superordinate level,bottom-up vs. top-down approach,cognitive grammar,culture,embodiment,LANGUAGE & culture,LINGUISTICS -- Research,metaphor,NARRATIVE inquiry (Research method),words vs. concepts}
}

@article{peirsman_corpus-based_2015,
  title = {The Corpus-Based Identification of Cross-Lectal Synonyms in Pluricentric Languages},
  volume = {20},
  issn = {13846655},
  doi = {10.1075/ijcl.20.1.03pei},
  abstract = {This article discusses a corpus-based method for the automatic identification of synonyms across different varieties of the same language. This method, based on the paradigm of distributional semantics, quantifies semantic similarity on the basis of contextual similarity in two comparable corpora. In two case studies for Dutch and German, we show that it automatically identifies the correct synonym for 31\% and 25\% of the target words, respectively. A manual error analysis moreover indicates that many additional synonyms are very close in the distributional model, while most other distributional neighbours are semantically related to the target word along other dimensions than synonymy. On the basis of these results, we argue that distributional-semantic methods can play a crucial role in the further evolution of corpus-based lexical semantics to a more quantitative discipline.},
  timestamp = {2015-07-09T02:36:27Z},
  number = {1},
  journaltitle = {International Journal of Corpus Linguistics},
