  pages = {559--575},
  keywords = {Bali,Cosmology,culture,Placemaking,Pluralistic},
  file = {Nirarta Samadhi_2001_The urban design of a Balinese town.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Nirarta Samadhi_2001_The urban design of a Balinese town.pdf:application/pdf}
}

@article{cienki_metaphor_2005,
  title = {Metaphor in the ‘‘{{Strict Father}}’’ and ‘‘{{Nurturant Parent}}’’ Cognitive Models: {{Theoretical}} Issues Raised in an Empirical Study},
