@article{launder_corpora_2010,
  title = {Corpora for {{Language Planning}} in {{Indonesia}}},
  rights = {Copyright (c) 2014 PROCEEDING},
  url = {http://journal.unwidha.ac.id/index.php/proceeding/article/view/318},
  abstract = {Corpora for Language Planning in Indonesia},
  timestamp = {2014-10-12T02:00:45Z},
  langid = {english},
  journaltitle = {Proceedings of Seminar Internasional PIBSI XXXII},
  author = {Launder, Allan F.},
  urldate = {2014-10-12},
  date = {2010},
  pages = {26--49},
  note = {This essay asserts that a large electronic corpus of Bahasa Indonesia should be 
constructed to provide data for the study of the Indonesian language and 
which could be of use in areas such as language planning, linguistics, applied 
linguistics and computational linguistics. Because corpus linguistics is still not 
well established in Indonesian linguistic circles, it presents an overview of what 
a corpus is, and of corpus typologies suitable for such a purpose. It then goes 
on to look at the essential features that such a corpus should have for it to be 
maximally useful for such a purpose. The most important issues are how large 
the corpus should be, and how it should be planned so that its composition 
was representative of the Indonesian language as a whole. It also deals with 
the issue of copyright. The construction of such a corpus would be of huge 
benefit to many, but it is a massive undertaking and one that would require 
the involvement of a large number of experts in different areas. It is hoped 
that this simple essay can mark the beginning of a consensus that would help 
make it a reality.

Keywords
corpora, corpus linguistics, Bahasa Indonesia, language planning, corpus design, 
corpus size, corpus representativeness, balance, source texts, online sources, 
published sources, spoken versus written corpora, corpus types, general corpus, 
specialized corpus, static corpus, dynamic (monitor) corpus, synchronic corpus, 
natural language processing (NLP), regional languages in Indonesia, indigenous 
languages, copyright.}
}

