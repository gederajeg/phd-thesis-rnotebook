  journaltitle = {Linguistics},
  shortjournal = {Linguistics},
  author = {Kutscher, Silvia and Schultze-Berndt, Eva},
  urldate = {2014-03-30},
  date = {2007-09},
  pages = {983--1028},
  keywords = {GERMAN language,LANGUAGE & languages,linguistics,PARTS of speech,SEMANTICS,VERBS (Grammar)}
}

