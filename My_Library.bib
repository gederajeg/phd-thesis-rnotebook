  pages = {69--88},
  keywords = {compounding,conceptual blending,conceptual metaphor,conceptual metonymy,figurative meaning},
  file = {Onysko_2014_Figurative processes in meaning interpretation.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Onysko_2014_Figurative processes in meaning interpretation.pdf:application/pdf}
}

