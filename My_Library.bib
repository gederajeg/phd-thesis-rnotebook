@article{crocket_shame_1979,
  title = {Shame, Resentment, Envy, and Pity — {{A}} Semantic Analysis},
  volume = {47},
  issn = {0024-3841},
  doi = {10.1016/0024-3841(79)90076-7},
  abstract = {This article offers an analysis of differences and similarities in the meanings of four Russian lexical entries on the basis of morphological and syntactic data. The four entries are STYD ‘shame’, OBID ‘resentment’, ZAVID ‘envy’, and ŽAL ‘pity’. The analysis demonstrates the validity of Fillmore's distinction between meaning proper and associated presuppositions, and it also demonstrates the pertinence of associated case frames and the viability of case categories proposed by Fillmore. The only additional concepts needed to capture the semantic relationship between the four entries appear to be the ones represented by the oppositions Inferior/Superior, Self/Non-Self, and Property/Activity. The analysis suggests the hypothesis that these oppositions constitute core concepts in the lexicon of psychological phenomena in Russian and perhaps in other languages as well.},
