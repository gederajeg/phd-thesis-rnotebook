@article{eddington_why_2002,
  title = {Why Quantitative?},
  volume = {40},
  issn = {00243949},
  doi = {10.1515/ling.2002.009},
  abstract = {An introduction to a special issue on quantitative approaches to Spanish linguistics.  Although the contributors to this issue deal with a broad range of phenomena from a number of different approaches, they are united in their use of quantitative methods.  In several respects, quantitative research is an important part of linguistic theorizing.  Quantifying a specific issue obligates the researcher to consider a wide range of data, thereby increasing the reliability of the analysis.  Data from quantitative studies are also critical in the testing of empirical hypotheses, particularly those having to do with human linguistic cognition.  Moreover, quantitative studies are in a better position to reflect people's actual use of language than analyses that do not use quantitative data.},
