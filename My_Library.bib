@article{renouf_filling_2013,
  title = {Filling the Gaps: {{Using}} the {{WebCorp Linguist}}'s {{Search Engine}} to Supplement Existing Text Resources},
  volume = {18},
  issn = {13846655},
  doi = {10.1075/ijcl.18.2.01ren},
  shorttitle = {Filling the Gaps},
  abstract = {The aim of this paper is to demonstrate the functionality of the WebCorp Linguist's Search Engine (WebCorpLSE) by comparing it with two other freely-available online data resources: Google and the Corpus of Contemporary American English (COCA). In particular, the paper looks at how WebCorpLSE can fill gaps in these sources. It begins by contextualising the WebCorpLSE project in relation to its parent project, WebCorp, and other 'web as corpus' initiatives. It then outlines the advantages of WebCorpLSE over Google search, before turning to a comparison of WebCorpLSE with COCA. Comparisons are made by matching extracts of textual and graphical output generated by each system. It is shown that WebCorpLSE provides information not available via the other search mechanisms, and that it can thus, in accordance with Mair's (2007) argument for a combined approach to diachronic text study, usefully serve not only as a stand-alone tool, but as a complementary resource.},
  timestamp = {2015-07-27T01:31:03Z},
  number = {2},
  journaltitle = {International Journal of Corpus Linguistics},
  shortjournal = {International Journal of Corpus Linguistics},
  author = {Renouf, Antoinette and Kehoe, Andrew},
