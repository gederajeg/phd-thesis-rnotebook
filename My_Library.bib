  pages = {199--244},
  file = {Moore_2006_Space-to-time mappings and temporal concepts.pdf:/Users/Primahadi/Documents/_Zotero Digital Library/Journal Article/Moore_2006_Space-to-time mappings and temporal concepts.pdf:application/pdf}
}

